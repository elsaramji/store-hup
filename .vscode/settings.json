{
    "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit",
        "source.fixAll": "explicit"
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off",
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first"
    },
    "redhat.telemetry.enabled": true,
    "files.autoSave": "afterDelay",
    "editor.minimap.enabled": false,
    "explorer.confirmDelete": false,
    "chatgpt.lang": "en",
    "[cpp]": {
        
        "editor.wordBasedSuggestions": "off",
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true
    },
    "[cpp][dart]": {
        "editor.wordBasedSuggestions": "off"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "explorer.confirmDragAndDrop": false,
    "dart.openDevTools": "flutter",
    "workbench.colorTheme": "Palenight (Mild Contrast)",
    "workbench.settings.applyToAllProfiles": [
        "editor.fontSize",
        "dartImport.excludeGeneratedFiles"
    ],
    "editor.fontSize": 18,
    "dart.debugExternalPackageLibraries": true,
    "dart.debugSdkLibraries": false,
    "editor.inlineSuggest.suppressSuggestions": true,
    "cody.commandCodeLenses": true,
    "cody.experimental.ollamaChat": true,
    "cody.useContext": "embeddings",
    "symbols.hidesExplorerArrows": false,
    "workbench.iconTheme": "material-icon-theme",
    "avdmanager.sdkPath": "c:\\Android",
    "dart.flutterAdbConnectOnChromeOs": true,
    "dart-import-formatter.enableDartImportFormatter": false,
    "codeium.enableConfig": {
        "*": true,
        "markdown": true,
        "plaintext": true
    },
    "[gradle]": {

    },
    "dart.warnWhenEditingFilesOutsideWorkspace": false,
    "workbench.editorAssociations": {
        "*.pdf": "pdf.view"
    },
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "git.ignoreRebaseWarning": true,
    "pieces.cloudCapabilities": "Blended",
    "pieces.telemetry": true,
